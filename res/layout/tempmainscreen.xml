<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tempmainscreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background2"
    tools:context="${relativePackage}.${activityClass}" >
    

    
   	<TextView
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="temp main"
   	    
   	    />
   	<Button 
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="homepage"
   	    android:id="@+id/temphomebtn"
   	    />
   	
   	<Button 
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="profile"
   	    android:id="@+id/tempprofilebtn"
   	    />
   	<Button 
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="conversation"
   	    android:id="@+id/tempconversationbtn"
   	    />
   	<Button 
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="Session History"
   	    android:id="@+id/tempsessionbtn"
   	    />
	
 	<Button 
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="User Settings"
   	    android:id="@+id/tempuserbtn"
   	    />
 	<Button 
   	    android:layout_width="fill_parent"
   	    android:layout_height="wrap_content"
   	    android:text="log out"
   	    android:id="@+id/templogout"
   	    />

</LinearLayout>